{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url"
    }
}